{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "script"
  },
  "globals": {
    "QUOTES": "readonly",
    "generateQuoteMapping": "readonly",
    "CalendarApp": "writable"
  },
  "rules": {
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "no-console": "off",
    "semi": ["error", "always"],
    "quotes": ["error", "single", { "avoidEscape": true }],
    "indent": ["error", 4],
    "no-multiple-empty-lines": ["error", { "max": 2 }],
    "eol-last": ["error", "always"]
  }
}
